{
    "name": "phoetik/pluguin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pluguin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pluguin\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sina Radmanesh",
            "email": "sina@webbax.dev"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "illuminate/container": "^8.83",
        "illuminate/config": "^8.83",
        "illuminate/database": "^8.83",
        "illuminate/support": "^8.83",
        "illuminate/filesystem": "8.83",
        "illuminate/events": "8.83"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "1.8.x-dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse src tests"
    }
}
